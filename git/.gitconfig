[user]
	email = andrewwong998@gmail.com
	name = Andrew Wong
[core]
	excludesfile = ~/.gitignore_global
	editor = nvim
	pager = diff-so-fancy
[color]
	status = auto
[alias]
	co = checkout
	st = status
	pom = push origin master
[color "diff-highlight"]
	oldNormal = red bold
	oldHighlight = red bold 52
	newNormal = green bold
	newHighlight = green bold 22
[color "diff"]
	meta = 11
	frag = magenta bold
	commit = yellow bold
	old = red bold
	new = green bold
	whitespace = red reverse
